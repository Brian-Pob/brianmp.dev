---
const title =
  "Survey | Investigating Developers' Preference for Learning Resources in the ChatGPT Era.";
const description =
  'This survey is designed to understand the role of AI chatbots, such as ChatGPT, in the landscape of software engineering and software development education. The survey focuses on how developers, students, and other individuals involved in software development perceive and utilize AI chatbots in comparison to other learning resources. ';
const image =
  'https://media.discordapp.net/attachments/644338284092456973/1158837510831611944/Preference_Learning_1.png?ex=652ed688&is=651c6188&hm=7fe9b5f07f2f9b8b3411c4208ee9efd340ec7d17140f308e66df6fb51740c341&=';
const url = 'https://brianmp.dev/external/survey';
const surveyURL = 'https://fsu.qualtrics.com/jfe/form/SV_6sAAmrlGo1PiUNo';
---

<!doctype html>
<html lang='en'>
  <head>
    <meta charset='UTF-8' />
    <meta name='viewport' content='width=device-width, initial-scale=1.0' />

    <!-- Open Graph Meta Data Placeholders -->
    <meta property='og:title' content={title} />
    <meta property='og:description' content={description} />
    <meta property='og:image' content={image} />
    <meta property='og:url' content={url} />
    <meta property='og:type' content='article' />

    <!-- Twitter Card Meta Data Placeholders -->
    <meta name='twitter:card' content='summary_large_image' />
    <meta name='twitter:title' content={title} />
    <meta name='twitter:description' content={description} />
    <meta name='twitter:image' content={image} />
    <meta name='twitter:url' content={url} />

    <!-- Page Title -->
    <title>Redirecting...</title>
  </head>
  <body>
    <p>Redirecting...</p>
    <p>
      If you are not redirected automatically, please <a href={surveyURL}
        >click here</a
      >.
    </p>
    <!-- JavaScript Redirect -->
    <script>
      // Set the URL you want to redirect to
      const redirectTo =
        'https://fsu.qualtrics.com/jfe/form/SV_6sAAmrlGo1PiUNo';

      // Redirect after a delay
      setTimeout(function () {
        window.location.href = redirectTo;
      }, 10000);
    </script>
  </body>
</html>
